<?xml version="1.0"?>
<robot name="mr2">
  <!-- Define intermediate links and base -->
  <link name="world_link"/>
  <link name="base_link_0"/>
  <link name="base_link_1"/>

  <link name="base">
    <visual>
      <origin xyz="0.0 0.0 0.2" />
      <geometry>
        <box size="1.0 1.0 0.4" />
      </geometry>
      <material name="dark_blue">
        <color rgba="0.0 0.0 0.5 1.0"/> <!-- Dark Blue -->
      </material>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="1.0" />
      <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1" />
    </inertial>
  </link>

  <!-- Define prismatic joints for x and y translations -->
  <joint name="x" type="prismatic">
    <limit lower="-5" upper="5" effort="10" velocity="1.0"/>
    <parent link="world_link"/>
    <child link="base_link_0"/>
    <axis xyz="1 0 0"/>
    <visual>
      <material name="yellow">
        <color rgba="1.0 1.0 0.0 1.0"/> <!-- Yellow -->
      </material>
    </visual>
  </joint>

  <joint name="y" type="prismatic">
    <limit lower="-5" upper="5" effort="10" velocity="1.0"/>
    <parent link="base_link_0"/>
    <child link="base_link_1"/>
    <axis xyz="0 1 0"/>
    <visual>
      <material name="yellow">
        <color rgba="1.0 1.0 0.0 1.0"/> <!-- Yellow -->
      </material>
    </visual>
  </joint>

  <!-- Define continuous joint for yaw rotation -->
  <joint name="theta" type="continuous">
    <parent link="base_link_1"/>
    <child link="base"/>
    <axis xyz="0 0 1"/>
    <visual>
      <material name="yellow">
        <color rgba="1.0 1.0 0.0 1.0"/> <!-- Yellow -->
      </material>
    </visual>
  </joint>

  <!-- Define additional links and joints as needed -->
  <link name="clavicle_right">
    <visual>
      <origin xyz="0.0 0.5 0.0" />
      <geometry>
        <box size="0.3 1.0 0.3" />
      </geometry>
      <material name="dark_blue">
        <color rgba="0.0 0.0 0.5 1.0"/> <!-- Dark Blue -->
      </material>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="0.5" />
      <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01" />
    </inertial>
  </link>

  <joint name="clavicle_right_yaw" type="revolute">
    <parent link="base"/>
    <child link="clavicle_right"/>
    <origin xyz="0.3 0.0 0.4" rpy="1.5708 0.0 0.0" />
    <axis xyz="0.0 1.0 0.0"/>
    <limit lower="-1.5708" upper="1.5708" effort="0.5" velocity="0.5"/>
    <visual>
      <material name="yellow">
        <color rgba="1.0 1.0 0.0 1.0"/> <!-- Yellow -->
      </material>
    </visual>
  </joint>

  <link name="clavicle_left">
    <visual>
      <origin xyz="0.0 0.5 0.0" />
      <geometry>
        <box size="0.3 1.0 0.3" />
      </geometry>
      <material name="dark_blue">
        <color rgba="0.0 0.0 0.5 1.0"/> <!-- Dark Blue -->
      </material>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="0.5" />
      <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01" />
    </inertial>
  </link>

  <joint name="clavicle_left_roll" type="revolute">
    <parent link="base"/>
    <child link="clavicle_left"/>
    <origin xyz="-0.3 0.0 0.4" rpy="1.5708 0.0 0.0" />
    <axis xyz="0.0 1.0 0.0"/>
    <limit lower="-1.5708" upper="1.5708" effort="0.5" velocity="0.5"/>
    <visual>
      <material name="yellow">
        <color rgba="1.0 1.0 0.0 1.0"/> <!-- Yellow -->
      </material>
    </visual>
  </joint>

  <link name="shoulder_right">
    <visual>
      <origin xyz="0.0 0.35 0.0" />
      <geometry>
        <box size="0.3 0.7 0.3" />
      </geometry>
      <material name="dark_blue">
        <color rgba="0.0 0.0 0.5 1.0"/> <!-- Dark Blue -->
      </material>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="0.5" />
      <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01" />
    </inertial>
  </link>

  <joint name="shoulder_right_yaw" type="revolute">
    <parent link="clavicle_right"/>
    <child link="shoulder_right"/>
    <origin xyz="0.0 0.85 0.15" rpy="1.5708 0.0 0.0" />
    <axis xyz="0.0 0.707 -0.707"/>
    <limit lower="-1.5708" upper="1.5708" effort="0.5" velocity="0.5"/>
    <visual>
      <material name="yellow">
        <color rgba="1.0 1.0 0.0 1.0"/> <!-- Yellow -->
      </material>
    </visual>
  </joint>

  <link name="upperarm_right">
    <visual>
      <origin xyz="0.0 0.35 0.0" />
      <geometry>
        <box size="0.3 0.7 0.3" />
      </geometry>
      <material name="dark_blue">
        <color rgba="0.0 0.0 0.5 1.0"/> <!-- Dark Blue -->
      </material>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="0.5" />
      <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01" />
    </inertial>
  </link>

  <joint name="upperarm_right_pitch" type="revolute">
    <parent link="shoulder_right"/>
    <child link="upperarm_right"/>
    <origin xyz="0.0 0.7 0.0" />
    <axis xyz="0.0 0.0 -1.0"/>
    <limit lower="-1.5708" upper="1.5708" effort="0.5" velocity="0.5"/>
    <visual>
      <material name="yellow">
        <color rgba="1.0 1.0 0.0 1.0"/> <!-- Yellow -->
      </material>
    </visual>
  </joint>

  <link name="forearm_right">
    <visual>
      <origin xyz="0.0 0.25 0.0" />
      <geometry>
        <box size="0.3 0.5 0.3" />
      </geometry>
      <material name="dark_blue">
        <color rgba="0.0 0.0 0.5 1.0"/> <!-- Dark Blue -->
      </material>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="0.5" />
      <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01" />
    </inertial>
  </link>

  <joint name="forearm_right_yaw" type="revolute">
    <parent link="upperarm_right"/>
    <child link="forearm_right"/>
    <origin xyz="0.0 0.7 0.0" />
    <axis xyz="1.0 0.0 0.0"/>
    <limit lower="-1.5708" upper="1.5708" effort="0.5" velocity="0.5"/>
    <visual>
      <material name="yellow">
        <color rgba="1.0 1.0 0.0 1.0"/> <!-- Yellow -->
      </material>
    </visual>
  </joint>

</robot>
